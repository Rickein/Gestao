#Tarefas
############# 


#Verificar todas as Tarefas
###
GET http://localhost:8000/Api/Tarefas HTTP/1.1

#Verificar tarefa especifica
###
GET http://localhost:8000/Api/Tarefas/1 HTTP/1.1


#Verificar Tarefas proximas do Prazo
###
GET http://localhost:8000/Api/Prazo HTTP/1.1


#Verificar Tarefas proximas do Prazo por Equipe
###
GET http://localhost:8000/Api/Tarefas/Prazo/1 HTTP/1.1

#Inserir nova tarefa
###
POST http://localhost:8000/Api/tarefas/novaTarefa HTTP/1.1
content-type: application/json

 {
  "Tarefa" :{"solicitante_tarefa": "solicitante teste", "assunto_tarefa": "assunto teste" ,
             "descricao_tarefa": "descricao teste", "prioridade_tarefa": "Baixa",
             "tempo_estimado_tarefa": "01:30", "prazo_tarefa": "18/07/2024 12:00"}
}

#Alterar tarefa
###
PATCH  http://localhost:8000/Api/tarefas/4 HTTP/1.1
content-type: application/json

 {
  "Tarefa" :{"solicitante_tarefa": "solicitante alterado", "assunto_tarefa": "assunto alterado" ,
             "descricao_tarefa": "descricao alterado", "prioridade_tarefa": "Alta",
             "tempo_estimado_tarefa": "01:30", "prazo_tarefa": "18/07/2024 13:00"}
}

#Remover tarefa
###
DELETE  http://localhost:8000/Api/Tarefa/4 HTTP/1.1


